<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.boke_houtai.dao.TWOCommentMapper">

    <insert id="saveTWOComment" parameterType="com.example.boke_houtai.pojo.TWOComment">
        insert  into twocomment(commentText,commentTime,twoUserName,userName,OneCommentID,state) values (${commentText},${commentTime},${twoUserName},${userName},${OneCommentID},${state});
