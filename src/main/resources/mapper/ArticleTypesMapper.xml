<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.boke_houtai.dao.ArticleTypesMapper">
    <insert id="saveArtileTypl" parameterType="ArticleTypes" >
        INSERT INTO articletypes (articleTypeName,describ,state,parentCategory) VALUES(#{articleTypeName},#{describ},#{state},#{parentCategory});
    </insert>

    <select id="findAll" resultType="ArticleTypes">
        select * from articletypes;
    </select>
</mapper>